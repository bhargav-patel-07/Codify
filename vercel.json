{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend-mig/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-mig/$1"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  },
  "installCommand": "pip install -r requirements.txt"
}
